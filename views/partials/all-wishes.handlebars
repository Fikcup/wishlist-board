<div class="wishes">
    {{#each wishes}}
    <article class="wish">
        <h2>
            <a href="/post/{{id}}">{{title}}</a>
        </h2>
        <div>
            Posted by {{User.username}}
        </div>
        <div class="wish-body">
            {{body}}
        </div>
    </article>
</div>